# vim: set ft=sh :

base="$GOPATH/src/github.com/jmoiron"
mkdir -p $base
cd $base

clone() {
    if [ ! -d "$1" ]; then
        git clone git@github.com:jmoiron/${1}.git
        return
    fi
    # XXX: pushd/popd not available in sh
    cd $1
    git pull
    cd ..
}

clone sqlx
clone modl
clone jigo
clone monet
clone gowiki
clone cm

