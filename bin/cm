#!/bin/bash

DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"
ARCH=$(uname -m)
case $(uname) in

    Darwin)
        $DIR/cm.osx $@
        ;;
    *)
        if [ "$ARCH" = "x86_64" ]; then
            $DIR/cm.linux $@
        else
            $DIR/cm.linux-i386 $@
        fi
        ;;
esac
